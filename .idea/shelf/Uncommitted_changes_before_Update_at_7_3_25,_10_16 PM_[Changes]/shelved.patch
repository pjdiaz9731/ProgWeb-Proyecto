<+>/* Global Styles */\n* {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n}\n\nbody {\n    font-family: 'Arial', sans-serif;\n    background-color: #FFFFFF; /* Blanco */\n    color: #333;\n}\n\n/* Header Styles */\nheader {\n    background-color: #0E7490; /* Azul oscuro */\n    color: white;\n    padding: 20px 0;\n}\n\nnav {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    max-width: 1200px;\n    margin: 0 auto;\n    padding: 0 20px;\n}\n\nnav h1 {\n    font-size: 1.8rem;\n    font-weight: bold;\n}\n\nnav ul {\n    list-style-type: none;\n    display: flex;\n}\n\nnav ul li {\n    margin-left: 20px;\n}\n\nnav ul li a {\n    color: white;\n    text-decoration: none;\n    font-size: 1rem;\n    font-weight: 500;\n}\n\nnav ul li a:hover {\n    text-decoration: underline;\n}\n\n.profile-icon {\n    font-size: 2rem;\n}\n\n/* Main Styles */\nmain {\n    padding: 40px 0;\n}\n\n.search-section {\n    display: flex;\n    justify-content: center;\n    gap: 15px;\n    margin-bottom: 40px;\n}\n\n.search-section input, .search-section button {\n    padding: 10px;\n    font-size: 1rem;\n    border-radius: 5px;\n    border: 1px solid #ccc;\n}\n\n.search-section input:focus, .search-section button:focus {\n    outline: none;\n    border-color: #0E7490; /* Azul oscuro */\n}\n\n.search-section button {\n    background-color: #FBBF24; /* Amarillo */\n    color: white;\n    cursor: pointer;\n    transition: background-color 0.3s;\n}\n\n.search-section button:hover {\n    background-color: #F97316; /* Naranja */\n}\n\n.search-section input {\n    border-color: #10B981; /* Verde */\n}\n\n.search-section input:focus {\n    border-color: #10B981; /* Verde */\n}\n\n/* Popular Flights Section */\n.popular-flights {\n    text-align: center;\n    margin-bottom: 40px;\n}\n\n.popular-flights h2 {\n    font-size: 1.6rem;\n    margin-bottom: 20px;\n    color: #0E7490; /* Azul oscuro */\n}\n\n.flights-container {\n    display: flex;\n    justify-content: center;\n    gap: 20px;\n}\n\n.flight-card {\n    background-color: #FFFFFF; /* Blanco */\n    padding: 20px;\n    border-radius: 10px;\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n    width: 200px;\n    text-align: center;\n    font-weight: bold;\n    transition: transform 0.3s;\n    border: 2px solid #FBBF24; /* Amarillo */\n}\n\n.flight-card:hover {\n    transform: translateY(-5px);\n}\n\n.flight-card:active {\n    transform: translateY(2px);\n}\n\n/* Recent Searches Section */\n.recent-searches {\n    text-align: center;\n}\n\n.recent-searches h2 {\n    font-size: 1.6rem;\n    margin-bottom: 20px;\n    color: #0E7490; /* Azul oscuro */\n}\n\n#recent-searches-container {\n    display: flex;\n    justify-content: center;\n    gap: 20px;\n}\n\n/* Footer Styles */\nfooter {\n    background-color: #0E7490; /* Azul oscuro */\n    color: white;\n    padding: 20px 0;\n    text-align: center;\n    margin-top: 40px;\n    \n}\n\nsvg {\n    color: white;\n    margin: 10px;\n}\n\n/* Button Secondary (Green) */\n.button-secondary {\n    background-color: #10B981; /* Verde */\n    color: white;\n    padding: 10px 20px;\n    border-radius: 5px;\n    border: none;\n    font-size: 1rem;\n    cursor: pointer;\n    transition: background-color 0.3s;\n}\n\n.button-secondary:hover {\n    background-color: #1E40AF; /* Azul */\n}\n\n/* Responsive Styles */\n@media (max-width: 768px) {\n    nav h1 {\n        font-size: 1.5rem;\n    }\n\n    nav ul {\n        flex-direction: column;\n        align-items: center;\n    }\n\n    nav ul li {\n        margin-left: 0;\n        margin-bottom: 10px;\n    }\n\n    .search-section {\n        flex-direction: column;\n        align-items: center;\n    }\n\n    .flights-container {\n        flex-direction: column;\n    }\n\n    .flight-card {\n        width: 100%;\n    }\n\n    #recent-searches-container {\n        flex-direction: column;\n    }\n}\n
