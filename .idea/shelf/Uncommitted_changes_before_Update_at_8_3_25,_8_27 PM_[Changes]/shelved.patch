<+>/* General Reset */\n* {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n}\n\n/* Cabecera */\nheader {\n    background-color: #1a1a1a;\n    padding: 20px;\n    color: #fff;\n}\n\nheader h1 {\n    font-size: 2.5rem;\n    text-align: center;\n    margin-bottom: 15px;\n}\n\n#nav-list {\n    display: flex;\n    justify-content: center;\n    list-style-type: none;\n    padding: 0;\n}\n\n#nav-list li {\n    margin: 0 20px;\n}\n\n#nav-list li a {\n    color: #fff;\n    text-decoration: none;\n    font-size: 1.2rem;\n    transition: color 0.3s;\n}\n\n#nav-list li a:hover {\n    color: #f1c40f;\n}\n\n.profile-icon {\n    position: absolute;\n    top: 20px;\n    right: 20px;\n}\n\n/* Sección de búsqueda */\n.search-section {\n    display: flex;\n    justify-content: center;\n    margin-top: 50px;\n    gap: 15px;\n}\n\n.search-section input, .search-section button {\n    padding: 10px;\n    font-size: 1rem;\n    border: 1px solid #ccc;\n    border-radius: 5px;\n}\n\n.search-section button {\n    background-color: #1a73e8;\n    color: white;\n    cursor: pointer;\n}\n\n.search-section button:hover {\n    background-color: #0c58c1;\n}\n\n/* Carrusel */\n.carousel-section {\n    margin-top: 50px;\n    text-align: center;\n}\n\n.carousel-section h2 {\n    font-size: 2rem;\n    margin-bottom: 20px;\n}\n\n.carousel-inner img {\n    height: 500px;\n    object-fit: cover;\n}\n\n.carousel-control-prev-icon, .carousel-control-next-icon {\n    background-color: #000;\n}\n\n/* Destinos populares */\n.popular-flights {\n    margin-top: 50px;\n    text-align: center;\n}\n\n.popular-flights h2 {\n    font-size: 2rem;\n    margin-bottom: 30px;\n}\n\n.flights-container {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n    gap: 20px;\n}\n\n.flight-card {\n    background-size: cover;\n    background-position: center;\n    width: 250px;\n    height: 250px;\n    color: white;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    font-size: 1.5rem;\n    font-weight: bold;\n    border-radius: 8px;\n    text-shadow: 2px 2px 5px rgba(0, 0, 0, 0.7);\n    transition: transform 0.3s;\n}\n\n.flight-card:hover {\n    transform: scale(1.1);\n}\n\n/* Busquedas recientes */\n.recent-searches {\n    margin-top: 50px;\n    text-align: center;\n}\n\n.recent-searches h2 {\n    font-size: 2rem;\n    margin-bottom: 20px;\n}\n\n/* Footer */\nfooter {\n    background-color: #1a1a1a;\n    color: white;\n    padding: 20px;\n    text-align: center;\n}\n\nfooter p {\n    font-size: 1rem;\n}\n\nfooter a {\n    margin: 0 10px;\n    color: white;\n    font-size: 1.5rem;\n    text-decoration: none;\n    transition: color 0.3s;\n}\n\nfooter a:hover {\n    color: #f1c40f;\n}\n\n/* Responsividad */\n@media (max-width: 768px) {\n    .search-section {\n        flex-direction: column;\n        align-items: center;\n    }\n\n    .flights-container {\n        flex-direction: column;\n    }\n\n    .flight-card {\n        width: 100%;\n        height: 200px;\n    }\n}\n
